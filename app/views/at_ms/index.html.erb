<p id="notice"><%= notice %></p>

<h1>Atms</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Long</th>
      <th>Lat</th>
      <th>Bank</th>
      <th>Network</th>
      <th>Location</th>
      <th>City</th>
      <th>Terminals</th>
      <th>Blindness assistance</th>
      <th>Dolars</th>
      <th>Street</th>
      <th>Street number</th>
      <th>Second street</th>
      <th>Neighborhood</th>
      <th>Commune</th>
      <th>Code</th>
      <th>Zipcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atms.each do |atm| %>
      <tr>
        <td><%= atm.id %></td>
        <td><%= atm.long %></td>
        <td><%= atm.lat %></td>
        <td><%= atm.bank %></td>
        <td><%= atm.network %></td>
        <td><%= atm.location %></td>
        <td><%= atm.city %></td>
        <td><%= atm.terminals %></td>
        <td><%= atm.blindness_assistance %></td>
        <td><%= atm.dolars %></td>
        <td><%= atm.street %></td>
        <td><%= atm.street_number %></td>
        <td><%= atm.second_street %></td>
        <td><%= atm.neighborhood %></td>
        <td><%= atm.commune %></td>
        <td><%= atm.code %></td>
        <td><%= atm.zipcode %></td>
        <td><%= link_to 'Show', atm %></td>
        <td><%= link_to 'Edit', edit_atm_path(atm) %></td>
        <td><%= link_to 'Destroy', atm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Atm', new_atm_path %>
